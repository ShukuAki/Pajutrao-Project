    <style>
        html, body {
            margin: 0;
            padding: 0;
            height: 100%;
            overflow-x: hidden;
        }

        .content-wrapper {
            display: flex;
            flex-direction: column;
            height: 100vh;
            overflow-y: scroll;
            position: relative; /* Necessary for sliding panel */
        }

            .content-wrapper img {
                width: 80%;
                height: auto;
            }

        .quiz-home-container {
            background-color: #fff;
            padding: 30px;
            border-radius: 10px;
            box-shadow: 0px 0px 15px rgba(0, 0, 0, 0.1);
            width: 400px;
            text-align: center;
            margin: 0 auto;
            margin-top: 100px;
        }

        h1 {
            font-size: 2em;
            margin-bottom: 20px;
        }

        .info {
            margin-bottom: 20px;
            font-size: 1.1em;
        }

            .info p {
                margin: 10px 0;
            }

        .start-btn {
            background-color: #4CAF50;
            color: white;
            padding: 10px 20px;
            border: none;
            border-radius: 5px;
            font-size: 1.2em;
            cursor: pointer;
        }

            .start-btn:hover {
                background-color: #45a049;
            }

        .quiz-panel {
            position: fixed;
            top: 0;
            right: -85%;
            width: 85%;
            height: 100%;
            background-color: #f9f9f9;
            box-shadow: -2px 0 5px rgba(0,0,0,0.2);
            transition: transform 0.5s ease-in-out;
            padding: 20px;
            overflow-y: auto;
            z-index: 1000;
            margin-top: 40px; /* Top margin of 10px */
        }

            .quiz-panel.open {
                transform: translateX(-100%);
            }

        .quiz-items {
            list-style-type: none;
            padding: 0;
        }

            .quiz-items li {
                padding: 15px;
                background-color: #fff;
                margin-bottom: 10px;
                box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            }

                .quiz-items li:hover {
                    background-color: #e0e0e0;
                }

        .choices {
            margin-top: 10px;
        }

        .choice {
            background-color: #eee;
            padding: 10px;
            margin-bottom: 5px;
            cursor: pointer;
            border-radius: 5px;
            font-weight: bold;
        }

            .choice:hover {
                background-color: #ccc;
            }

        .close-btn {
            background-color: #f44336;
            color: white;
            padding: 10px;
            border: none;
            border-radius: 5px;
            cursor: pointer;
            float: right;
        }

            .close-btn:hover {
                background-color: #d32f2f;
            }

        .submit-btn {
            background-color: #008CBA;
            color: white;
            padding: 15px 30px;
            border: none;
            border-radius: 5px;
            font-size: 1.2em;
            cursor: pointer;
            display: block;
            margin: 30px auto;
        }

            .submit-btn:hover {
                background-color: #007bb5;
            }
    </style>

